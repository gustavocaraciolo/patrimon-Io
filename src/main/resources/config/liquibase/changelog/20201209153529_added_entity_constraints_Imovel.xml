<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.9.xsd">
    <!--
        Added the constraints for entity Imovel.
    -->
    <changeSet id="20201209153529-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="user_id"
                                 baseTableName="imovel"
                                 constraintName="fk_imovel_user_id"
                                 referencedColumnNames="id"
                                 referencedTableName="jhi_user"/>

        <addForeignKeyConstraint baseColumnNames="contas_receber_id"
                                 baseTableName="imovel"
                                 constraintName="fk_imovel_contas_receber_id"
                                 referencedColumnNames="id"
                                 referencedTableName="contas_receber"/>

        <addForeignKeyConstraint baseColumnNames="contas_pagar_id"
                                 baseTableName="imovel"
                                 constraintName="fk_imovel_contas_pagar_id"
                                 referencedColumnNames="id"
                                 referencedTableName="contas_pagar"/>

    </changeSet>
</databaseChangeLog>
